cmake_minimum_required(VERSION 3.16)

project(googletest-external NONE)

include(ExternalProject)
ExternalProject_Add(googletest
  URL                         "${propeller_gtest_download_url}"
  SOURCE_DIR                  "${propeller_gtest_src_dir}"
  BINARY_DIR                  "${propeller_gtest_build_dir}"
  CONFIGURE_COMMAND           ""
  BUILD_COMMAND               ""
  INSTALL_COMMAND             ""
  TEST_COMMAND                ""
  DOWNLOAD_EXTRACT_TIMESTAMP  TRUE
)
